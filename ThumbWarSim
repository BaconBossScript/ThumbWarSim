})

_G.train1 = true
_G.train2 = true
_G.train3 = true
_G.train4 = true
function train1()
     while _G.train1 == true do
        game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("Knit"):WaitForChild("Services"):WaitForChild("FightService"):WaitForChild("RF"):WaitForChild("PlayTool"):InvokeServer()
     wait(0)
    end
end

function train2()
    while _G.train2 == true do
        local args = {
            [1] = "Biceps"
        }
        
        game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("Knit"):WaitForChild("Services"):WaitForChild("FightService"):WaitForChild("RF"):WaitForChild("AutoTrain"):InvokeServer(unpack(args))
      wait(0)
   end
end

function train3()
    while _G.train3 == true do
        local args = {
            [1] = "Hands"
        }
        
        game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("Knit"):WaitForChild("Services"):WaitForChild("FightService"):WaitForChild("RF"):WaitForChild("AutoTrain"):InvokeServer(unpack(args)) 
     wait(0)
   end
end

function train4()
    while _G.train4 == true do
        local args = {
            [1] = "Barbells"
        }
        
        game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("Knit"):WaitForChild("Services"):WaitForChild("FightService"):WaitForChild("RF"):WaitForChild("AutoTrain"):InvokeServer(unpack(args))        
     wait(0)
   end
end

TpTab:AddToggle({
	Name = "Auto Train Knuckle",
	Default = false,
	Callback = function(Value)
		_G.train1 = Value
        train1()
	end    
})
 
