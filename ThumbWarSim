	Name = "Auto Train Bicep",
	Default = false,
	Callback = function(Value)
		_G.train2 = Value
        train2()
	end    
})

TpTab:AddToggle({
	Name = "Auto Train Hand",
	Default = false,
	Callback = function(Value)
		_G.train3 = Value
        train3()
	end    
})

TpTab:AddToggle({
    Name = "Auto Train Barbell",
	Default = false,
	Callback = function(Value)
		_G.train4 = Value
        train4()
	end    
})

_G.rb = true 
function rb()
     while _G.rb == true do
        local args = {
            [1] = "Rebirth"
        }
        
        game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("Knit"):WaitForChild("Services"):WaitForChild("PlayerService"):WaitForChild("RF"):WaitForChild("FrameOpen"):InvokeServer(unpack(args))
     wait(0.5)
    end
end

TpTab:AddToggle({
	Name = "Auto Rebirth",
	Default = false,
	Callback = function(Value)
		_G.rb = Value
        rb()
	end    
})

 
_G.click = true
function click()
     while _G.click == true do
        game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("Knit"):WaitForChild("Services"):WaitForChild("FightService"):WaitForChild("RF"):WaitForChild("AddFight"):InvokeServer()
     wait(0)
    end
end

Tp2Tab:AddToggle({
	Name = "Auto Click Fight",
	Default = false,
	Callback = function(Value)
		_G.click = Value
        click()
	end    
})

_G.segg = "CyanGymEgg"
_G.egg = true
function egg()
     while _G.egg == true do   
        game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("Knit"):WaitForChild("Services"):WaitForChild("DataService"):WaitForChild("RF"):WaitForChild("EggAction"):InvokeServer("1xHatch", _G.segg)
        wait(0)
        game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("Knit"):WaitForChild("Services"):WaitForChild("DataService"):WaitForChild("RF"):WaitForChild("FinishHatch"):InvokeServer()
     wait(0)
    end
end

Tp3Tab:AddToggle({
	Name = "Auto Hatch",
	Default = false,
	Callback = function(Value)
		_G.egg = Value
        egg()
	end    
})

